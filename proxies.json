{
  "$schema": "http://json.schemastore.org/proxies",
  "proxies": {
    "get-file-by-curseforge": {
      "matchCondition": {
        "methods": [
          "GET",
          "HEAD"
        ],
        "route": "/api/curseforge/file/{projectId}/{fileId}"
      },
      "backendUri": "https://voxelauncher.azurewebsites.net/api/get-file-by-curseforge",
      "requestOverrides": {
        "backend.request.querystring.project": "{projectId}",
        "backend.request.querystring.file": "{fileId}"
      }
    },
    "get-metadata-by-curseforge": {
      "matchCondition": {
        "methods": [
          "GET"
        ],
        "route": "/api/curseforge/metadata/{projectId}/{fileId}"
      },
      "backendUri": "https://voxelauncher.blob.core.windows.net/curseforge/{projectId}/{fileId}.json"
    },
    "get-metadata-by-modid": {
      "matchCondition": {
        "methods": [
          "GET"
        ],
        "route": "/api/mods/metadata/{modId}/{version}"
      },
      "backendUri": "https://voxelauncher.blob.core.windows.net/mods/{modId}/{version}.json"
    },
    "get-file-by-modid": {
      "matchCondition": {
        "methods": [
          "GET"
        ],
        "route": "/api/mods/file/{modId}/{version}"
      },
      "backendUri": "https://voxelauncher.azurewebsites.net/api/get-file-by-modid",
      "requestOverrides": {
        "backend.request.querystring.modid": "{modId}",
        "backend.request.querystring.version": "{version}"
      }
    },
    "get-java": {
      "matchCondition": {
        "methods": [
          "GET"
        ],
        "route": "/api/jre/{os}/{arch}"
      },
      "backendUri": "https://voxelauncher.blob.core.windows.net/jre/jre-{os}-{arch}-1.8.0_51.lzma"
    }
  }
}